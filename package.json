{
  "name": "Quantum",
  "version": "1.0.0",
  "description": "A JavaScript SQL Editor",
  "author": "Cristiano de Barros",
  "main": "main.js",
  "scripts": {
    "start": "webpack && electron ./app/",
    "dev:webpack": "webpack --watch",
    "dev:start": "electron ./app/",
    "dist": "build",
    "dist:macos": "build --macos",
    "dist:linux": "build --linux",
    "dist:windows": "build --windows"
  },
  "build": {
    "appId": "com.electron.quantum",
    "linux": {
      "target": [
        "deb"
      ]
    }
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "JavaScript",
    "SQL",
    "editor"
  ],
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "electron": "^1.4.1",
    "electron-builder": "^11.2.5",
    "webpack": "^1.14.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "brace": "^0.9.1",
    "dateformat": "^2.0.0",
    "jquery": "^3.1.1",
    "mssql": "^3.3.0",
    "nedb": "^1.8.0",
    "nprogress": "^0.2.0",
    "pg": "^6.1.2",
    "react": "^15.4.1",
    "react-ace": "^4.1.1",
    "react-dom": "^15.4.1",
    "split.js": "^1.1.2"
  }
}
